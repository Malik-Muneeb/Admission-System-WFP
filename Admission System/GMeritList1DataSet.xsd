                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        TOP (5) G_Candidate.G_FORM_NO, G_Candidate.G_CNAME, G_Candidate.G_FNAME, G_Candidate.G_CTEST, G_Candidate.G_INTERVIEW, 
                         ROUND((G_Candidate.G_CMATRIC / 4.0 + G_Candidate.G_CINTER / 5.0 + G_Candidate.G_CHAFIZ + G_Candidate.G_BS_CGPA) / 486.5 * 70 + 30 * (G_Candidate.G_CTEST / 60.0), 2) AS Expr1
FROM            G_Candidate INNER JOIN
                         Programme ON G_Candidate.PROG_ID = Programme.PROG_ID
WHERE        (G_Candidate.PROG_ID =
